/*
#include <iostream>
#include <limits>

void program5() {

    int a;
    std::cout << "vvedite odno  chislo \n";
    std::cin >> a;
    if (std::cin.fail())
    {
        std::cout << "\nInput error!!!";

        std::cin.clear();

        std::cin.ignore(std::numeric_limits<std::streamsize>::max(), '\n');
        return;
    }


    if (a >= 10 && a < 100 && a % 2 == 0 || a > -100 && a <= -10 && a % 2==0 ) {
        std::cout << "Da";
    } else {
        std::cout << "Net";
    }
}
    int main()
    {
        bool answer;
        program5();
        std::cout << "\nOne more? (1 - yes, 0 - no): ";
        std::cin >> answer;
        if (answer)
        {
            main();
        }
        return 0;
    }
